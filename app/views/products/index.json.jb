@products.map do |product|
  {
    id: product.id,
    name: product.name,
    img_url: product.img_url,
    price: product.price,
    total: product.total,
    is_discounted?: product.is_discounted?,
    description: product.description,
    inventory: product.inventory,
    created_at: product.readable_created_at,
    updated_at: product.readable_updated_at
 

  }
end
